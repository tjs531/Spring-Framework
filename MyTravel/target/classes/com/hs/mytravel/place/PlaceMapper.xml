<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hs.mytravel.place.PlaceMapper">
	<insert id="insPlace" parameterType="PlaceVO">
		INSERT INTO t_place
		(place_name, p_lat, p_long)
		VALUES 
		(#{place_name}, #{p_lat}, #{p_long})
	</insert>
	
</mapper>